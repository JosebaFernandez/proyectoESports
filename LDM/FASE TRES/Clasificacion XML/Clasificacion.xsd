<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <!-- Definición del elemento jornadas -->
  <xs:element name="jornadas">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="jornada"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- Definición del elemento jornada -->
  <xs:element name="jornada">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="N_JORNADA" type="xs:integer"/>
        <xs:element name="EQUIPO_LOCAL" type="xs:string"/>
        <xs:element name="EQUIPO_VISITANTE" type="xs:string"/>
        <xs:element name="RESULTADO" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>
